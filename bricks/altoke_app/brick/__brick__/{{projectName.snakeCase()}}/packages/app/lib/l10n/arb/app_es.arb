  "deleteTaskButtonTooltip": "Eliminar tarea \"{taskTitle}\"",
  "@deleteTaskButtonTooltip": {
    "type": "text",
    "description": "Tooltip del botón de eliminar tarea",
    "placeholders": {
      "taskTitle": {
        "type": "String",
        "description": "Título de la tarea",
