{
  "@@locale": "es",
  "appTitle": "{{projectName.titleCase()}}",
  "@appTitle": {
    "description": "Título de la aplicación"
  },
  "genericRetryButtonLabel": "Reintentar",
  "@genericRetryButtonLabel": {
    "type": "text",
    "description": "Etiqueta para un botón genérico de reintento",
    "placeholders": {}
  },
  "appInitializationErrorMessage": "Hubo un problema al intentar lanzar la aplicación",
  "@appInitializationErrorMessage": {
    "type": "text",
    "description": "Mensaje mostrado cuando falla la inicialización de la aplicación",
    "placeholders": {}
  },
  "homeAppBarTitle": "Ejemplos",
  "@homeAppBarTitle": {
    "type": "text",
    "description": "Título de la cabecera de la pantalla de ejemplos",
    "placeholders": {}
  },
  "counterExampleListTileTitle": "Contador",
  "@counterExampleListTileTitle": {
    "type": "text",
    "description": "Título de la tarjeta de ejemplo del contador",
    "placeholders": {}
  },
  "counterAppBarTitle": "Contador",
  "@counterAppBarTitle": {
    "description": "Título de la cabecera de la pantalla del contador"
  },
  "counterPushTimesMessage": "{pushTimes, plural, =0{Aún no has pulsado el botón} =1{Has pulsado el botón una vez} other{Has pulsado el botón {pushTimes} veces}}.",
  "@counterPushTimesMessage": {
    "description": "Mensaje con el número de veces que se ha pulsado el botón",
    "placeholders": {
      "pushTimes": {
        "type": "int",
        "format": "compact"
      }
    }
  },
  "counterIncrementButtonTooltip": "Incrementar",
  "@counterIncrementButtonTooltip": {
    "description": "Tooltip del botón de incrementar"
  },
  "counterResetButtonTooltip": "Reiniciar",
  "@counterResetButtonTooltip": {
    "description": "Tooltip del botón de reiniciar"
