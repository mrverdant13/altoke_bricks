    "type": "text",
    "description": "Etiqueta del botón de crear tarea",
    "placeholders": {}
  },
  "createTaskSuccessMessage": "Tarea creada exitosamente",
  "@createTaskSuccessMessage": {
    "type": "text",
    "description": "Mensaje mostrado cuando se crea una tarea con éxito",
    "placeholders": {}
  },
  "createTaskTitleEmptyError": "El título no puede estar vacío",
