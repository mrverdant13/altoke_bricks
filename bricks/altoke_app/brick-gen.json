{
  "targetRelativePath": "./{{projectName.snakeCase()}}",
  "replacements": [
    {
      "from": "altoke-app",
      "to": "{{projectName.kebabCase()}}"
    },
    {
      "from": "altoke_app",
      "to": "{{projectName.snakeCase()}}"
    },
    {
      "from": "Altoke App",
      "to": "{{projectName.titleCase()}}"
    },
    {
      "from": "An Altoke App.",
      "to": "{{projectDescription}}"
    }
  ],
  "lineDeletions": [
    {
      "filePath": "./{{projectName.snakeCase()}}/packages/app/lib/l10n/arb/app_en.arb",
      "ranges": [
        {
          "start": 51,
          "end": 158
        }
      ]
    },
    {
      "filePath": "./{{projectName.snakeCase()}}/packages/app/lib/l10n/arb/app_es.arb",
      "ranges": [
        {
          "start": 51,
          "end": 158
        }
      ]
    }
  ]
}
