{
  "targetRelativePath": "./{{project_name.snakeCase()}}",
  "replacements": [
    {
      "from": "packages/app/android",
      "to": "packages/app/{{#android_organization}}android{{/android_organization}}"
    },
    {
      "from": "com/mrverdant13/altoke_app",
      "to": "{{{android_application_identifier_as_path}}}"
    },
    {
      "from": "com.mrverdant13.altoke_app",
      "to": "{{android_application_identifier}}"
    },
    {
      "from": "altoke-app",
      "to": "{{project_name.kebabCase()}}"
    },
    {
      "from": "altoke_app",
      "to": "{{project_name.snakeCase()}}"
    },
    {
      "from": "Altoke App",
      "to": "{{project_name.titleCase()}}"
    },
    {
      "from": "An Altoke App.",
      "to": "{{project_description}}"
    }
  ],
  "lineDeletions": [
    {
      "filePath": "./{{project_name.snakeCase()}}/packages/app/lib/l10n/arb/app_en.arb",
      "ranges": [
        {
          "start": 51,
          "end": 158
        }
      ]
    },
    {
      "filePath": "./{{project_name.snakeCase()}}/packages/app/lib/l10n/arb/app_es.arb",
      "ranges": [
        {
          "start": 51,
          "end": 158
        }
      ]
    }
  ]
}
