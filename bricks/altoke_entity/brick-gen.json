{
  "targetRelativePath": "./{{entity_singular.snakeCase()}}",
  "replacements": [
    {
      "from": "altoke_common",
      "to": "common"
    },
    {
      "from": "Representation of different altoke entity variants.",
      "to": "{{package_description}}"
    },
    {
      "from": "(DmAltokeEntity)|(EAltokeEntity)|(FAltokeEntity)",
      "to": "AltokeEntity"
    },
    {
      "from": "(DmNewAltokeEntity)|(ENewAltokeEntity)|(FNewAltokeEntity)",
      "to": "NewAltokeEntity"
    },
    {
      "from": "(DmPartialAltokeEntity)|(EPartialAltokeEntity)|(FPartialAltokeEntity)",
      "to": "PartialAltokeEntity"
    },
    {
      "from": "(DmOptional)|(EOptional)|(FOptional)",
      "to": "Optional"
    },
    {
      "from": "altokeEntity",
      "to": "{{entity_singular.camelCase()}}"
    },
    {
      "from": "altoke entity",
      "to": "{{entity_singular.lowerCase()}}"
    },
    {
      "from": "AltokeEntity",
      "to": "{{entity_singular.pascalCase()}}"
    },
    {
      "from": "Altoke entity",
      "to": "{{entity_singular.sentenceCase()}}"
    },
    {
      "from": "altoke_entity",
      "to": "{{entity_singular.snakeCase()}}"
    },
    {
      "from": "Altoke Entity",
      "to": "{{entity_singular.titleCase()}}"
    }
  ]
}
