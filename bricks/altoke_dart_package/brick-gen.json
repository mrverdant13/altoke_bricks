{
  "targetRelativePath": ".",
  "replacements": [
    {
      "from": "\\b(REQUIREMENTS.md)\\b",
      "to": "{{^requirements_met}}REQUIREMENTS.md{{/requirements_met}}"
    },
    {
      "from": "build.yaml",
      "to": "{{#use_code_generation}}build.yaml{{/use_code_generation}}"
    },
    {
      "from": "An altoke Dart package.",
      "to": "{{package_description}}"
    },
    {
      "from": "altoke_dart_package",
      "to": "{{#requirements_met}}{{package_name.snakeCase()}}{{/requirements_met}}"
    },
    {
      "from": "AltokeDartPackage",
      "to": "{{package_name.pascalCase()}}"
    },
    {
      "from": "Altoke Dart Package",
      "to": "{{package_name.titleCase()}}"
    }
  ]
}
